menu "Apps" folder {
    prog "Terminal" - sakura
    prog "Web Browser" - librewolf
    prog "Connection Manager" - cmst
    prog "Code Lite" - codelite
    prog "Discord" - discord
    prog "Edit IceWM Menu" - sakura -m -e "nano ~/.icewm/menu"
    prog "Restart" - sakura -e "sudo reboot"
    prog "Power Off" - sakura -e "sudo poweroff"

}

